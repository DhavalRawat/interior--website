<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- bootstrap cdn link -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <!-- aos cdn link -->
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">

    <!-- custom styles -->
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" href="responsive.css">

    <title>Interiorx - Contact</title>
</head>

<body>
    <!-- topbar start -->
    <!-- topbar end -->





    <!-- header start -->
   <%- include('header'); -%>
    <!-- header end -->





    <!-- breadcrumb start -->
    <div class="container-fluid breadcrumb-area">
        <div class="container-lg">
            <div class="title">
                <h2>Sign up</h2>
            </div>

            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page">sign up</li>
                </ol>
            </nav>
        </div>
    </div>
    <!-- breadcrumb end -->





    <!-- contact section start -->
    <div class="dhaval">

    <div class="container contact">
        <div class="contact-wrap">
            <div class="contact-address">
                <div class="contact-title">
                    <h1>Let's Connect!</h1>
                   
                </div>
              
            </div>

            <div class="contact-form">
                <form method="post" enctype="multipart/form-data">
                    <div class="input-field">
                        <input type="text" placeholder="Your name" name="txt1" autocomplete="off" required>
                    </div>

                    <div class="input-field">
                        <input type="email" placeholder="Your email" name="txt2" autocomplete="off" required>
                    </div>

                    <div class="input-field">
                        <input type="text" placeholder="Your password" name="txt3" autocomplete="off" required>
                    </div>

                    <div class="input-field">
                        <input type="number" placeholder="Phone number" name="txt4" autocomplete="off" required>
                    </div>

                    <div class="input-field">
                        <input type="file" name="file12">
                    </div>

              
                 

                    <div class="button-field">
                        <button type="submit">submit</button>
                    </div>

                   
                </form>
            </div>
        </div>
    </div>
    <!-- contact section end -->





    <!-- footer section start -->
    <%- include('footer'); -%>


    <!-- footer section end -->







    <!-- bootstrap cdn link -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
    </script>

    <!-- aos cdn link -->
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

    <!-- custom js link -->
    <script src="script.js"></script>
</body>

</html>